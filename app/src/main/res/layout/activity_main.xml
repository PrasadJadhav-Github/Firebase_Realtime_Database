<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activitys.MainActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#90EE90"
        android:gravity="center_vertical"
        android:padding="8dp"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        app:title="Display"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageViewVersionList"
        android:layout_width="78dp"
        android:layout_height="78dp"
        android:layout_marginRight="260dp"
        android:src="@drawable/versionlist"
        android:layout_marginTop="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:text="Version List"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:layout_marginLeft="18dp"
        app:layout_constraintEnd_toEndOf="@+id/imageViewVersionList"
        app:layout_constraintStart_toStartOf="@+id/imageViewVersionList"
        app:layout_constraintTop_toBottomOf="@+id/imageViewVersionList" />

</androidx.constraintlayout.widget.ConstraintLayout>